for(let i = 0; i<5; i++){
    text += "The number is "+i+" <br>"
    alert(textd)
}

var time = prompt("que horas sao:")

if (time<10){
    greeting = "good morning";
}
else if (time<20){
    greeting= "good afternoon"
}
else{
    greeting = "good night"
}
alert(greeting)

var text = 0
for(let i = 0; i<5; i++){
    text += "The number is "+i+" <br>"
    alert(text)
}

let i = 0;
var text = 0

while(i<10){
    text +="The number is "+i;
    i++;
    alert(text)
}
